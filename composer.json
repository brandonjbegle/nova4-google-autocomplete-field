{
  "name": "brandonjbegle/nova4-google-autocomplete-field",
  "description": "This Nova Package it's a Field that allows you to Autocomplete addresses with Google Place API and get the full metadata (lat and long, etc).",
  "keywords": [
    "laravel",
    "nova",
    "field",
    "google places api",
    "address autocomplete"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "BrandonJBegle\\GoogleAutocomplete\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BrandonJBegle\\GoogleAutocomplete\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
